# Configuración para Desarrollo - MiR Online
# Este archivo puede ser compartido con el equipo de desarrollo

# Secret temporal para desarrollo (NO usar en producción)
API_SECRET=dev_temp_secret_replace_in_prod

# URLs de la API
API_BASE_URL=https://api.mironline.io/api/v1
API_STUDENTS_LOGIN=/students/login  
API_STUDENTS_PROFILE=/students/today
API_STUDENTS_EGP=/students/egp
API_STUDENTS_EXTRA_ATTEMPT=/students/extra-attempt
API_STUDENTS_LOGOUT=/students/logout

# Configuración de red
NETWORK_TIMEOUT=30
CERTIFICATE_PINNING_ENABLED=false

# Debug mode habilitado para desarrollo
DEBUG_LOGGING_ENABLED=true